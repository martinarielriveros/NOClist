generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_DB_URL")
}

enum SeverityLevel {
  LOW
  MEDIUM
  HIGH
  CRITICAL
}


model logModel {
    id    Int     @id @default(autoincrement())
    level SeverityLevel
    type  String
    message String
    origin String
    timestamp DateTime @default(now())
}